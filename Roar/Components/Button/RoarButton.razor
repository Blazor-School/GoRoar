@namespace Roar.Components

<RoarDependency>
    <script type="module" src="@Assets[InternalPaths.ComposeWebAwesomeComponentJsPath("button")]"></script>
</RoarDependency>
<wa-button variant="@Variant">@ChildContent</wa-button>

@code {
    [Parameter]
    public Variant Variant { get; set; } = default;

    [Parameter]
    public RenderFragment? ChildContent { get; set; }
}
